            android:layout_below="@+id/educ_gyr2"
            />

        <TextView
            android:id="@+id/skill"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/address"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="70dp"
            android:fontFamily="@font/adamina"
            android:gravity="left"
            android:text="SKILLS"
            android:textColor="#F5DEB3"
            android:textSize="13sp"
            android:textStyle="bold" />



        <TextView
            android:id="@+id/skill1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="skill1..."
            android:textColor="#F5DEB3"
            android:gravity="left"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="30dp"
            android:textSize="11dp"
            android:layout_below="@+id/skill"
            />

        <TextView
            android:id="@+id/skill2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="skill2..."
            android:textColor="#F5DEB3"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="30dp"
            android:textSize="11dp"
            android:layout_below="@+id/skill1"
            />

        <TextView
            android:id="@+id/skill3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="skill3..."
            android:textColor="#F5DEB3"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="30dp"
            android:textSize="11dp"
            android:layout_below="@+id/skill2"
            />

        <TextView
            android:id="@+id/skill4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="skill4..."
            android:textColor="#F5DEB3"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="30dp"
            android:textSize="11dp"
