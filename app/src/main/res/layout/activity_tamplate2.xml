<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center|top"
    android:background="#FFFFFF"
    android:orientation="vertical"
    >




    <RelativeLayout
        android:id="@+id/lineard"
        android:layout_width="wrap_content"
        android:layout_height="680dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/tmp2"
        android:orientation="vertical">

        <TextView
            android:id="@+id/fname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/adamina"
            android:gravity="center"
            android:text="Name..."
            android:layout_marginLeft="16dp"
            android:layout_marginVertical="30dp"
            android:textColor="#F5DEB3"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/address"
            android:layout_width="110sp"
            android:maxWidth="110sp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Address..."
            android:textColor="#F5DEB3"
            android:gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="-6dp"
            android:textSize="10dp"
            android:layout_below="@+id/fname"
            />

        <TextView
            android:id="@+id/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Phone..."
            android:textColor="#F5DEB3"
            android:gravity="center"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="-65dp"
            android:textSize="13dp"
            android:layout_below="@+id/address"
            />

        <TextView
            android:id="@+id/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Email..."
            android:textColor="#F5DEB3"
            android:gravity="center"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="140dp"
            android:textSize="13dp"
            android:layout_below="@+id/phone"
            />

        <TextView
            android:id="@+id/summ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Summary..."
            android:textColor="#F5DEB3"
            android:gravity="center"
            android:layout_marginTop="17dp"
            android:layout_marginLeft="135dp"
            android:textSize="10dp"
            android:layout_below="@+id/email"
            />

        <TextView
            android:id="@+id/personal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/summ"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="80dp"
            android:fontFamily="@font/adamina"
            android:gravity="left"
            android:text="EXPERIENCE"
            android:textColor="@color/black"
            android:textSize="13dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/cname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="150dp"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/adamina"
            android:gravity="left"
            android:text="company name..."
            android:textColor="@color/black"
            android:textSize="10dp"

            android:layout_below="@+id/cpos"/>

        <TextView
            android:id="@+id/ccity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="address..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="150dp"
            android:textSize="10dp"
            android:layout_below="@+id/cleave"
            />

        <TextView
            android:id="@+id/cpos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:layout_below="@+id/personal"
            android:text="position..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="140dp"
            android:textSize="10dp"
            android:textStyle="bold"

            />

        <TextView
            android:id="@+id/cjoin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Join..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="150dp"
            android:textSize="10dp"
            android:layout_below="@+id/cname"
            />



        <TextView
            android:id="@+id/cleave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Leave..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="-15dp"
            android:layout_marginLeft="180dp"
            android:textSize="10dp"
            android:layout_below="@+id/cjoin"
            />


        <TextView
            android:id="@+id/cname2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="150dp"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/adamina"
            android:gravity="left"
            android:text="company name..."
            android:textColor="@color/black"
            android:textSize="10dp"

            android:layout_below="@+id/cpos2"/>

        <TextView
            android:id="@+id/ccity2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="address..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="150dp"
            android:textSize="10dp"
            android:layout_below="@+id/cleave2"
            />

        <TextView
            android:id="@+id/cpos2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:layout_below="@+id/ccity"
            android:text="position..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="140dp"
            android:textSize="10dp"
            android:textStyle="bold"

            />

        <TextView
            android:id="@+id/cjoin2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Join..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="150dp"
            android:textSize="10dp"
            android:layout_below="@+id/cname2"
            />



        <TextView
            android:id="@+id/cleave2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
