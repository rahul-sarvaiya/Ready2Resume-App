            android:textSize="10dp"
            android:layout_below="@+id/educ_major"

            />

        <TextView
            android:id="@+id/educ_city"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="City..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="-15dp"
            android:layout_marginLeft="80dp"
            android:textSize="10dp"
            android:layout_below="@+id/inst"
            />

        <TextView
            android:id="@+id/educ_major"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Major..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="20dp"
            android:textSize="10dp"
            android:layout_below="@+id/educ1"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/educ_gyr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Grad year..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="20dp"
            android:textSize="10dp"
            android:layout_below="@+id/educ_city"
            />

        <TextView
            android:id="@+id/educ_summ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Summary..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="20dp"
            android:textSize="10dp"
            android:layout_below="@+id/educ_gyr"
            />


        <TextView
            android:id="@+id/inst2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
            android:text="Institution..."
            android:textColor="@color/black"
            android:gravity="left"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="20dp"
            android:textSize="10dp"
            android:layout_below="@+id/educ_major2"

            />

        <TextView
            android:id="@+id/educ_city2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/adamina"
